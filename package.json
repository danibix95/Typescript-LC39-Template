{
  "name": "mia_template_service_name_placeholder",
  "version": "0.1.0",
  "description": "%CUSTOM_PLUGIN_SERVICE_DESCRIPTION%",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ignore-path=.gitignore --ext=ts",
    "lint:fix": "npm run lint -- --fix",
    "unit": "jest",
    "test": "npm run lint && npm run unit",
    "coverage": "npm run unit -- --coverage",
    "clean": "rm -rf ./dist",
    "check-types": "tsc --pretty --project tsconfig.json",
    "compile": "npm run check-types && swc src -d dist -s true",
    "build": "npm run clean && npm run compile",
    "dev": "jest --watch",
    "start": "lc39 dist/index.js",
    "start:local": "npm run compile && npm start -- --env-path ./default.env",
    "update-docker-version": "sed -i.bck \"s|version=\\\"[0-9]*\\.[0-9]*\\.[0-9]*.*\\\"|version=\\\"${npm_package_version}\\\"|\" Dockerfile",
    "update-changelog": "node ./scripts/update-changelog.js ${npm_package_version}",
    "version": "npm run update-changelog && npm run update-docker-version && rm -fr Dockerfile.bck && git add CHANGELOG.md Dockerfile",
    "install-precommit": "npx husky install"
  },
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@mia-platform/custom-plugin-lib": "^5.0.0"
  },
  "devDependencies": {
    "@mia-platform/eslint-config-mia": "^3.0.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.197",
    "@swc/jest": "^0.2.21",
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "chokidar": "^3.5.3",
    "eslint": "^8.17.0",
    "eslint-plugin-jest": "^26.5.3",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "json-schema-to-ts": "^2.5.3",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3"
  },
  "peerDependencies": {
    "@mia-platform/lc39": "^6.0.1"
  },
  "engines": {
    "node": ">=16"
  },
  "private": true
}
